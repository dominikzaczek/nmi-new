.container
    .row
        #carouselExampleCaptions.carousel.slide.shadowy(data-ride='carousel')
            ol.carousel-indicators
                li.active(data-target='#carouselfExampleCaptions' data-slide-to="0")
                li(data-target='#carouselfExampleCaptions' data-slide-to="0")
            .carousel-inner
                .carousel-item.active
                    img.d-block.w-100(src='/images/mic.png')
                    .carousel-caption.d-none.d-md-block
                        h5 Good vs bad approach
                .carousel-item
                    img.d-block.w-100(src='/images/ladder.png')
                    .carousel-caption.d-none.d-md-block
                        h5 Is learning how to sing difficult?
            .carousel-control-prev(href='#carouselExampleCaptions' role='button' data-slide='prev')
                span.carousel-control-prev-icon(arria-hidden='true')
                span.sr-only Previous
            .carousel-control-next(href='#carouselExampleCaptions' role='button' data-slide='next')
                span.carousel-control-next-icon(arria-hidden='true')
                span.sr-only Next
            



